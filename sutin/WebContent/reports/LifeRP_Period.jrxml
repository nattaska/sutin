	<queryString><![CDATA[select @rownum:=@rownum+1 rownum,name,tel,provincename,lifemonth,lifedate,province
from (
	select concat(ifnull(getParName(3,c.prefix),''),c.name) name,ifnull(c.tel,'-') tel,
       		ifnull(getParName(1,c.province),'-') provincename,	c.province ,
		ifnull(substr(l.lifemonth,4,2),'') lifemonth,lifemonth lifedate
	from customer c,life l
	where c.custid=l.custid
	and l.lifemonth is not null
	and l.active = 1
	and STR_TO_DATE(l.lifemonth,'%d/%m/%Y') between STR_TO_DATE($P{SDATE},'%d/%m/%Y') and STR_TO_DATE($P{EDATE},'%d/%m/%Y')
	and (-1 = $P{provinceId} or c.province = $P{provinceId})
	order by substr(l.lifemonth,4,2),c.name
) cl,(SELECT @rownum:=0) r]]></queryString>

